{
    "name": "icie",
    "version": "0.4.10",
    "publisher": "pustaczek",
    "engines": {
        "vscode": "^1.29.0"
    },
    "displayName": "ICIE",
    "description": "Competitive programming IDE-as-a-VS-Code-plugin",
    "categories": [
        "Other"
    ],
    "keywords": [
        "competitive",
        "ide",
        "codeforces",
        "olympiad",
        "informatics"
    ],
    "repository": "https://github.com/matcegla/icie",
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "icie.build",
                "title": "ICIE Build"
            },
            {
                "command": "icie.test",
                "title": "ICIE Test"
            },
            {
                "command": "icie.init",
                "title": "ICIE Init"
            },
            {
                "command": "icie.submit",
                "title": "ICIE Submit"
            },
            {
                "command": "icie.template.instantiate",
                "title": "ICIE File from Template"
            },
            {
                "command": "icie.manual.submit",
                "title": "ICIE Manual Submit"
            },
            {
                "command": "icie.test.view",
                "title": "ICIE Test view"
            },
            {
                "command": "icie.test.new",
                "title": "ICIE Add new test"
            },
            {
                "command": "icie.test.discoverer",
                "title": "ICIE Test Discoverer"
            },
            {
                "command": "icie.paste.pick",
                "title": "ICIE Paste pick"
            },
            {
                "command": "icie.external.terminal",
                "title": "ICIE Terminal"
            },
            {
                "command": "icie.init.existing",
                "title": "ICIE Init existing"
            }
        ],
        "keybindings": [
            {
                "command": "icie.init",
                "key": "alt+f11"
            },
            {
                "command": "icie.submit",
                "key": "alt+f12"
            },
            {
                "command": "icie.template.instantiate",
                "key": "alt+="
            },
            {
                "command": "icie.test.view",
                "key": "alt+0"
            },
            {
                "command": "icie.test.new",
                "key": "alt+-"
            },
            {
                "command": "icie.test.discoverer",
                "key": "alt+9"
            },
            {
                "command": "icie.paste.pick",
                "key": "alt+["
            },
            {
                "command": "icie.external.terminal",
                "key": "alt+t"
            }
        ]
    },
    "activationEvents": [
        "onCommand:icie.build",
        "onCommand:icie.test",
        "onCommand:icie.init",
        "onCommand:icie.init.existing",
        "onCommand:icie.submit",
        "onCommand:icie.template.instantiate",
        "onCommand:icie.manual.submit",
        "onCommand:icie.test.view",
        "onCommand:icie.test.new",
        "onCommand:icie.test.discoverer",
        "onCommand:icie.paste.pick",
        "onCommand:icie.external.terminal",
        "workspaceContains:/main.cpp"
    ],
    "badges": [],
    "markdown": "github",
    "qna": "marketplace",
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "scripts": {
        "make": "cd .. && make",
        "tsc": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "watch": "tsc -watch -p ./",
        "vscode:prepublish": "npm run make"
    }
}
